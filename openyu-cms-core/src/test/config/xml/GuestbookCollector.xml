<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<guestbookCollector>
    <id>org.openyu.cms.guestbook.vo.GuestbookCollector</id>
    <inquiry>
        <sorts>
            <names>
                <entry key="zh_TW">標題</entry>
                <entry key="zh_CN">标题</entry>
                <entry key="en_US">Titles</entry>
            </names>
            <id>titles</id>
            <order>
                <names/>
            </order>
        </sorts>
        <sorts>
            <names>
                <entry key="zh_TW">推薦</entry>
                <entry key="zh_CN">推荐</entry>
                <entry key="en_US">Recommend</entry>
            </names>
            <id>recommend</id>
            <order>
                <names/>
            </order>
        </sorts>
        <sorts>
            <names>
                <entry key="zh_TW">審核</entry>
                <entry key="zh_CN">审核</entry>
                <entry key="en_US">Checked</entry>
            </names>
            <id>checked</id>
            <order>
                <names/>
            </order>
        </sorts>
        <export>false</export>
    </inquiry>
    <defaultGuestbook>DEFAULT</defaultGuestbook>
    <guestbooks>
        <entry key="DEFAULT">
            <value>
                <id>DEFAULT</id>
                <titles/>
                <contents/>
                <replys/>
                <checked>false</checked>
                <recommend>false</recommend>
            </value>
        </entry>
    </guestbooks>
    <targetOptions>_balnk</targetOptions>
    <targetOptions>_self</targetOptions>
    <actionOptions>
        <names>
            <entry key="zh_TW">未知</entry>
            <entry key="zh_CN">未知</entry>
            <entry key="en_US">Unknown</entry>
        </names>
        <id>UNKNOWN</id>
    </actionOptions>
    <actionOptions>
        <names>
            <entry key="zh_TW">新增</entry>
            <entry key="zh_CN">新增</entry>
            <entry key="en_US">Insert</entry>
        </names>
        <id>INSERT</id>
    </actionOptions>
    <actionOptions>
        <names>
            <entry key="zh_TW">修改</entry>
            <entry key="zh_CN">修改</entry>
            <entry key="en_US">Update</entry>
        </names>
        <id>UPDATE</id>
    </actionOptions>
    <actionOptions>
        <names>
            <entry key="zh_TW">刪除</entry>
            <entry key="zh_CN">删除</entry>
            <entry key="en_US">Delete</entry>
        </names>
        <id>DELETE</id>
    </actionOptions>
    <logInquiry>
        <pagination>
            <pageSizes>10</pageSizes>
            <pageSizes>20</pageSizes>
            <pageSizes>50</pageSizes>
            <pageSizes>100</pageSizes>
        </pagination>
        <sorts>
            <names>
                <entry key="zh_TW">記錄日期</entry>
                <entry key="zh_CN">记录日期</entry>
                <entry key="en_US">Log Date</entry>
            </names>
            <id>logDate</id>
            <order>
                <names/>
            </order>
        </sorts>
        <sorts>
            <names>
                <entry key="zh_TW">使用者</entry>
                <entry key="zh_CN">用户</entry>
                <entry key="en_US">User ID</entry>
            </names>
            <id>userId</id>
            <order>
                <names/>
            </order>
        </sorts>
        <sorts>
            <names>
                <entry key="zh_TW">IP</entry>
                <entry key="zh_CN">IP</entry>
                <entry key="en_US">IP</entry>
            </names>
            <id>clientIp</id>
            <order>
                <names/>
            </order>
        </sorts>
        <orders>
            <names>
                <entry key="zh_TW">降冪</entry>
                <entry key="zh_CN">降幂</entry>
                <entry key="en_US">Descending</entry>
            </names>
            <id>DESC</id>
        </orders>
        <orders>
            <names>
                <entry key="zh_TW">升冪</entry>
                <entry key="zh_CN">升幂</entry>
                <entry key="en_US">Ascending</entry>
            </names>
            <id>ASC</id>
        </orders>
        <export>false</export>
    </logInquiry>
